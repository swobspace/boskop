<table class="table table-sm">
  <% responsibilities.each do |responsibility| %>
    <%= content_tag(:tr) do %>
      <td class=""><%= responsibility.responsibility_for.to_str %></td>
      <td class=""><%= responsibility.role %></td>
      <td class=""><%= responsibility.title %></td>
    <% end # content_tag %>
  <% end # each %>
</table>
