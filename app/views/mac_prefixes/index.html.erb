<h1><%= t('controller.mac_prefixes') %></h1>

<div data-controller="datatables">
  <table id="mac_prefixes" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th><%= t('attributes.oui') %></th>
      <th><%= t('attributes.vendor') %></th>
      <th class="nosort"></th>
    </tr>
  </thead>
  <tfoot class="search">
    <tr>
      <th></th>
      <th></th>
      <th class="search"></th>
    </tr>
  </thead>
  <tbody>

  <% @mac_prefixes.each do |mac_prefix| %>
    <%= content_tag_for(:tr, mac_prefix) do %>
      <td><%= mac_prefix.oui %></td>
      <td><%= mac_prefix.vendor %></td>
      <td class="nowrap">
        <%= show_link mac_prefix %>
        <%= edit_link mac_prefix %>
        <%= delete_link mac_prefix %>
      </td>
    <% end %>
  <% end %>
  </tbody>
  </table>
</div>

<br />

<%= back_link %>
<%= new_link MacPrefix %>
